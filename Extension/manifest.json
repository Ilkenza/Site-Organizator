{
  "manifest_version": 3,
  "name": "Site Organizer",
  "version": "1.0.0",
  "description": "Organize and categorize the websites you visit",
  "permissions": ["activeTab", "storage", "alarms"],
  "host_permissions": ["https://skacyhzljreaitrbgbte.supabase.co/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon-72x72.png",
      "48": "icons/icon-96x96.png",
      "128": "icons/icon-128x128.png"
    }
  },
  "icons": {
    "16": "icons/icon-72x72.png",
    "48": "icons/icon-96x96.png",
    "128": "icons/icon-128x128.png",
    "192": "icons/icon-192x192.png",
    "512": "icons/icon-512x512.png"
  }
}
