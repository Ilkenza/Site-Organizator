{
  "manifest_version": 3,
  "name": "Site Organizer",
  "version": "1.0.0",
  "description": "Organizuj i kategorišuj sajtove koje posećuješ",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "favicon16.png",
      "48": "favicon.ico",
      "180": "favicon180.png"
    }
  },
  "icons": {
    "16": "favicon16.png",
    "48": "favicon.ico",
    "180": "favicon180.png"
  }
}
